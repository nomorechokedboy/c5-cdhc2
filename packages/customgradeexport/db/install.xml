<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="local/customgradeexport/db" VERSION="20241213" COMMENT="XMLDB file for Custom Grade Export plugin"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:noNamespaceSchemaLocation="../../../lib/xmldb/xmldb.xsd"
>
  <TABLES>
    <TABLE NAME="local_customgradeexport_tpl" COMMENT="Template metadata for multiple templates">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="true"/>
        <FIELD NAME="type" TYPE="char" LENGTH="20" NOTNULL="true" SEQUENCE="false" COMMENT="Template type: quiz, assign, or course"/>
        <FIELD NAME="templateid" TYPE="char" LENGTH="100" NOTNULL="true" SEQUENCE="false" COMMENT="Unique template identifier"/>
        <FIELD NAME="name" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" COMMENT="User-friendly template name"/>
        <FIELD NAME="timecreated" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
        <FIELD NAME="timemodified" TYPE="int" LENGTH="10" NOTNULL="true" SEQUENCE="false"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
      <INDEXES>
        <INDEX NAME="type_templateid" UNIQUE="true" FIELDS="type, templateid"/>
      </INDEXES>
    </TABLE>
  </TABLES>
</XMLDB>
